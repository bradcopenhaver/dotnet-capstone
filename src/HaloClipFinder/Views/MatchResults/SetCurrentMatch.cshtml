@{
    Layout = "";
}
<h3>Match Results</h3>
@foreach (var team in ViewBag.CurrentTeams)
{
    <div>
        <h4>@team[0]</h4>
        @for(var i=1; i<team.length; i++)
        {
            <p>@team[i]</p>
        }
    </div>
}