@{
}
@foreach (var mEvent in ViewBag.MatchEvents)
{
    if (mEvent.EventName == "Medal")
    {
        <div>
            <p>medal: @mEvent.MedalId -- time: @mEvent.TimeSinceStart</p>
        </div>

    }
}